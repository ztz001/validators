{
  "name": "validator-util",
  "version": "0.0.6",
  "description": "字符校验工具库",
  "main": "index.js",
  "module": "es/index.js",
  "browser": "validator-util.js",
  "types": "es/index.d.ts",
  "typings": "index.d.ts",
  "files": [
    "es",
    "lib",
    "index.js",
    "index.d.ts",
    "validator-util.js",
    "README.md"
  ],
  "engines": {
    "node": ">=13.0.0"
  },
  "scripts": {
    "build": "jest && npm run lint && npm run clean && npm run build:es && npm run build:node && npm run build:web",
    "build:es": "tsc --project ./tsconfig.es.json",
    "build:node": "tsc --project ./tsconfig.node.json",
    "build:web": "webpack",
    "clean": "shx rm -rf es && shx rm -rf lib && shx rm -rf index.js && shx rm -rf validator-util.js",
    "lint": "eslint src/*.ts",
    "cwt": "concurrently -r -p '' -k 'npm run watch' 'npm run watch:test'",
    "test": "npm run pre-test && jest",
    "test:only": "jest",
    "pre-test": "npm run clean && npm run build:es",
    "watch": "tsc -w",
    "watch:test": "jest -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ztz2/validator-util.git"
  },
  "keywords": [
    "validator",
    "validator-util",
    "validation",
    "validate",
    "assert"
  ],
  "author": {
    "name": "ztz2",
    "email": "448941273@qq.com",
    "url": "https://github.com/ztz2"
  },
  "bugs": {
    "url": "https://github.com/ztz2/validator-util/issues"
  },
  "homepage": "https://github.com/ztz2/validator-util#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/runtime": "^7.13.10",
    "@types/jest": "29.2.4",
    "@types/node": "^10.12.10",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^29.4.3",
    "concurrently": "^8.0.1",
    "eslint": "^7.32.0",
    "jest": "^29.4.3",
    "lint-staged": "^10",
    "shx": "^0.3.4",
    "ts-jest": "29.0.3",
    "tsify": "^5.0.4",
    "tslib": "^2.5.0",
    "typescript": "^4.4.4",
    "babel-loader": "^9.1.2",
    "terser-webpack-plugin": "^5.3.7",
    "ts-loader": "^9.4.2",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "license": "MIT"
}
